}

@article{dasilvaDeathsDueCOVID192021,
  title = {Deaths Due to {{COVID-19}} in a State of Northeastern {{Brazil}}: Spatiotemporal Distribution, Sociodemographic and Clinical and Operational Characteristics.},
  author = {da Silva, Weslei Melo and Brito, Paula Dos Santos and de Sousa, Giana Gislanne da Silva and Santos, Livia Fernanda Siqueira and da Silva, Janiel Conceicao and Costa, Ana Cristina Pereira de Jesus and Pascoal, Livia Maia and Santos, Floriacy Stabnow and Graepp Fontoura, Iolanda and Lobato, Jaisane Santos Melo and Fontoura, Volmar Morais and Pereira, Ana Lucia Fernandes and Dos Santos, Leonardo Hunaldo and Santos Neto, Marcelino},
  options = {useprefix=true},
