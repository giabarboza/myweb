  author = {Woody, Spencer and Tec, Mauricio and Dahan, Maytal and Gaither, Kelly and Lachmann, Michael and Fox, Spencer J. and Meyers, Lauren Ancel and Scott, James and {The University of Texas at Austin COVID-19 Modeling Consortium}},
