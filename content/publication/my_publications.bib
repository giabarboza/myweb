	file = {Ali et al. - 2021 - The impact of COVID-19 on the U.S. child care mark.pdf:C\:\\Users\\gbarboza\\Zotero\\storage\\DJ58MGB3\\Ali et al. - 2021 - The impact of COVID-19 on the U.S. child care mark.pdf:application/pdf},
