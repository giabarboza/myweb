	file = {Hamidi and Zandiatashbar - 2021 - Compact development and adherence to stay-at-home .pdf:C\:\\Users\\gbarboza\\Zotero\\storage\\6RL6G2WI\\Hamidi and Zandiatashbar - 2021 - Compact development and adherence to stay-at-home .pdf:application/pdf},
